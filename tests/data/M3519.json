          "@type" : "oa:Annotation",
          "motivation" : "oad:transcribing",
          "resource" : {
            "@type" : "cnt:ContentAsText",
            "cnt:chars" : "գեղք և գաւառք անմարդ մնային, և այլ ոչ շինեցան մինչ"
          },
